{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "lint:check": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "build": "ttsc -p .",
    "migration:create": "npx knex migrate:make $NAME -x ts",
    "migration:up": "npx knex migrate:latest",
    "migration:down": "npx knex migrate:rollback",
    "seed:run": "npx knex seed:run --specific=$NAME"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint --cache --fix",
    "src/**/*.{ts, md}": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "knex-stringcase": "^1.4.6",
    "pg": "^8.9.0",
    "querystring": "^0.2.1",
    "socket.io": "^4.6.1",
    "socket.io-adapter": "^2.5.2",
    "ttypescript": "^1.5.15",
    "type-fest": "^3.10.0",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/pg": "^8.6.6",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2"
  },
  "keywords": [],
